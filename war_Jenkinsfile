pipeline{
	agent any
	
	
	tools{
	maven 'maven_3.9.0'
	}
	
	stages{
		stage('SCM Checkout'){
			steps{
				checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Karthi-289/java-maven-war-app.git']])
			}
		}
		
		stage('Maven-Builed'){
			steps{
			sh 'mvn clean install'
			}
		}
		
		
	}
}
